{
    "name": "simoneddy/config",
    "description": "basic read only config class with file/dir loader and dot notation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
       "bin-dir": "bin"
    },
    "autoload": {
       "psr-4": {
          "Eddy\\Config\\": "src"
       }
    },
    "suggest": {
        "symfony/yaml": "Required for YAML support"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.3",
        "symfony/yaml": "^5.3",
        "phpspec/phpspec": "^7.4"
    },
    "require": {
        "php": ">=8.0"
    }
}
